{
  "name": "pdfequips-donate-btn",
  "type": "module",
  "version": "0.0.3",
  "description": "A modern, animated donate button component for Astro.js with multi-language support",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "astro",
    "donate",
    "button",
    "react",
    "typescript",
    "scss",
    "multilingual",
    "pdfequips"
  ],
  "author": "PDFEquips Team",
  "license": "MIT",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "build:lib": "tsc --outDir dist && npm run build:styles",
    "build:styles": "sass src/styles:dist --no-source-map && tailwindcss -i src/styles/input.css -o dist/styles.css --minify",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "astro": "^5.10.0",
    "lucide-react": "^0.518.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwindcss": "^4.1.10"
  },
  "devDependencies": {
    "sass": "^1.89.2"
  }
}